<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yc.lining.mapper.TypesinfoMapper" >
   <select id="selectAllTypes" resultType="Typesinfo">
      select typesid,typename from typesinfo
   </select>
   <resultMap type="Typesinfo" id="typeslist">
 		<id column="typesid" property="typesid" />
 		<result column="typesid" property="typesid"/>
 		<result column="typename" property="typename" />
 	</resultMap>
 	<select id="listAllType" resultMap="typeslist">
 		select * from typesinfo
  	</select>
  	<insert id="addType" parameterType="Typesinfo">
  		insert into typesinfo values(seq_types.nextval,#{typename})
  	</insert>
</mapper>
